{
  "name": "fs-gdrive",
  "version": "1.2.0",
  "description": "A file system-like interface for Google Drive operations in Node.js",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "google",
    "drive",
    "google drive",
    "gdrive",
    "fs",
    "file system",
    "fs-gdrive"
  ],
  "author": "Ahmed (wo6zz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wo6zz/fs-gdrive.git"
  },
  "bugs": {
    "url": "https://github.com/wo6zz/fs-gdrive/issues"
  },
  "homepage": "https://github.com/wo6zz/fs-gdrive#readme",
  "dependencies": {
    "@googleapis/drive": "^8.11.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.4.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
